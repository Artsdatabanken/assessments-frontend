@model SpeciesAssessment2021

@if (Model.Category.Contains("LC"))
{
    <h3>
        Hvorfor er ikke denne arten rødlistet?
    </h3>
    <p>
        Denne arten har ikke oppfylt noen av kriteriene for å rødlistes.
        Den er havner derfor i kategorien livskraftig LC.
    </p>
    <a class="button tertiary tinybutton" href="https://www.artsdatabanken.no/Rodliste/Metode#193097">
        Les om kriteriene her
        <span class="material-icons">
            keyboard_arrow_right
        </span>
    </a>
}





