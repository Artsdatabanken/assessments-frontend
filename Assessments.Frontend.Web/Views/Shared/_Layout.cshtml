    <title>@ViewData["Title"] - Assessments Frontend</title>
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="https://doi.test.artsdatabanken.no/style-reset.css" asp-append-version="true" />
    <link rel="stylesheet" href="https://doi.test.artsdatabanken.no/layout.css" asp-append-version="true" />
