@model MapViewModel;

@{
    //Map of Norway with svalbard
    //Region limits per 2018
    var seaAreas = new string[] { "Barentshavet nord og Polhavet", "Barentshavet sør", "Grønlandshavet", "Nordsjøen og Skagerrak", "Norskehavet" };
    var showOceanAreasInMap = Model.RegionOccurrences.Any(x => seaAreas.Any(y => x.Region.DisplayName() == y && (x.IsKnown || x.IsAssumedToday || x.IsAssumedInFuture)));
    var viewBoxValues = showOceanAreasInMap ? "0 0 172.13006 278.71942" : "0 100 172.13006 178.71942";

    string className(string name)
    {
        string className = "";
        var element = Model.RegionOccurrences.Where(x => x.Region.DisplayName() == name).FirstOrDefault();
        if (element != null)
        {
            if(Model.MapText == Constants.AlienSpecies2023KnownOrAssumedOccurrence)
            {
                if (element.IsKnown)
                {
                    className += " known_today";
                }else if (element.IsAssumedToday)
                {
                    className += " is_assumed";
                }
            }else if (Model.MapText == Constants.AlienSpecies2023AssumedInFutureOccurrence)
            {
                if (element.IsAssumedInFuture)
                {
                    className += " is_assumed";
                }    
            }                   
        }
        return className;      
    }

    void makeFormattedList(List<AlienSpeciesAssessment2023RegionOccurrence> listToShow)
    {
        for (var i = 0; i < listToShow.Count; i++){
            if (i + 1 < listToShow.Count){
            
                <span>@listToShow[i].Region.DisplayName(), </span>}
            else
            {
                <span>@listToShow[i].Region.DisplayName().</span>
            }
        }        
    }   
}

<div class="map-box-container">
    <div class="map-description">
        <b>@Model.MapText</b>
        <br>
        <br>
        <span>
            @{
                // Split list for map of today:
                List<AlienSpeciesAssessment2023RegionOccurrence> knownToday;
                List<AlienSpeciesAssessment2023RegionOccurrence> assumedToday;

                if (Model.MapText == Constants.AlienSpecies2023KnownOrAssumedOccurrence)
                {
                    knownToday = Model.RegionOccurrences.Where(x => x.IsKnown == true).ToList();
                    assumedToday = Model.RegionOccurrences.Where(x => x.IsAssumedToday == true && x.IsKnown == false).ToList();


                    <span>
                        <br>
                        <b>Kjent utbredelse</b>
                        <br>
                        <span is-visible="knownToday.Count == 0">Arten er ikke kjent i noen områder</span>
                        @{
                            makeFormattedList(knownToday);
                        }
                        <br>
                        <br>
                        <b>Antatt utbredelse</b>
                        <br>
                        <span is-visible="assumedToday.Count == 0 && knownToday.Count == 0">Det antas at arten ikke finnes i noen områder</span>
                        <span is-visible="assumedToday.Count == 0 && knownToday.Count > 0">Arten er antatt å kun finnes i de kjente områdene</span>
                        @{
                            makeFormattedList(assumedToday);
                        }
                    </span>
                }
                else
                {
                    makeFormattedList(@Model.RegionOccurrences);
                    <span is-visible="Model.RegionOccurrences.Count == 0">Det antas ingen utbredelse om 50 år <!-- TODO: Hide map? --></span>
                }
            }
        </span>
    </div>

    <div is-visible="Model.ShowLegend">
        <br>
        <table class="map-legend">
            <tr>
                <td class="indicator is_known_today"></td>
                <td>Kjent utbredelse</td>
            </tr>
            <tr>
                <td class="indicator is_assumed"></td>
                <td>Antatt utbredelse</td>
            </tr>
            <tr>
                <td class="indicator no_occurrence"></td>
                <td>Ikke kjent eller antatt</td>
            </tr>
        </table>
    </div>

