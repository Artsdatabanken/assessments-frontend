@using Assessments.Frontend.Web.Infrastructure

@inject AssessmentsApiClient _apiClient

@{
    ViewBag.Title = "Assessments api httpclient test";

    var categories = await _apiClient
        .Get<IList<AssessmentsApiClient.Category>>("redlist2015?$filter=length(Kategori) eq 2&$apply=groupby((Kategori))&orderby=Kategori");
}

<select asp-items="@new SelectList(categories.Select(x => x.Kategori))"></select>
