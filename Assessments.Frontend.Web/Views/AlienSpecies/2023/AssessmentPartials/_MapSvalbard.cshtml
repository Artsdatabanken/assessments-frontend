    string className(string name)
    {
        string className = "current";
        if (Model.RegionOccurrences.Any(x => x.Region.DisplayName() == name))
        {
            return Model.MapText switch
            {
                Constants.AlienSpecies2023KnownOccurrence => "known_today",
                Constants.AlienSpecies2023AssumedOccurrence => "is_assumed_today",
                Constants.AlienSpecies2023AssumedInFutureOccurrence => "is_assumed_in_future",
